{
  "name": "flickr-view",
  "version": "0.0.9",
  "description": "Sample app flickr photo viewing application.",
  "dependencies": {  
    "napa": "latest",
    "angular": "latest",
    "angular-route": "latest",
    "browserify": "latest"
  },
  "devDependencies": {
    "bower": "latest",
    "gulp": "latest",
    "karma": "~0.12.16",
    "karma-browserify": "latest",
    "karma-jasmine": "latest",
    "karma-firefox-launcher": "~0.1.3",
    "beefy": "latest"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "install": "napa; node_modules/.bin/browserify ./app/js/index.js > ./app/js/bundle.js",
    "start": "node_modules/.bin/browserify ./app/js/index.js > ./app/js/bundle.js; node_modules/.bin/beefy ./js/index.js:js/bundle.js --liv --cwd ./app"
  },
  "napa": {
    "angular": "angular/bower-angular",
    "angular-route": "angular/bower-angular-route"
  }

}
